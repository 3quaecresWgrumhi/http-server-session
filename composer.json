{
    "name": "amphp/http-server-session",
    "homepage": "https://github.com/amphp/http-server-session",
    "description": "Session management for Amp's HTTP server.",
    "license": "MIT",
    "authors": [
        {
            "name": "Aaron Piotrowski",
            "email": "aaron@trowski.com"
        },
        {
            "name": "Bob Weinand",
            "email": "bobwei9@hotmail.com"
        },
        {
            "name": "Niklas Keller",
            "email": "me@kelunik.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Amp\\Http\\Server\\Session\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Amp\\Http\\Server\\Session\\Test\\": "test"
        }
    },
    "require": {
        "php": ">=7.1",
        "amphp/amp": "^2",
        "amphp/http-server": "^2",
        "paragonie/constant_time_encoding": "^2.2",
        "amphp/sync": "^1",
        "amphp/cache": "^1"
    },
    "require-dev": {
        "amphp/redis": "^1",
        "amphp/phpunit-util": "^1.1",
        "phpunit/phpunit": "^8 || ^7",
        "amphp/http": "^1",
        "amphp/php-cs-fixer-config": "dev-master",
        "league/uri-schemes": "^1.1"
    },
    "minimum-stability": "RC",
    "suggest": {
        "amphp/redis": "Allows storing session data in Redis"
    },
    "scripts": {
        "test": "@php -dzend.assertions=1 -dassert.exception=1 ./vendor/bin/phpunit",
        "code-style": "@php ./vendor/bin/php-cs-fixer fix"
    }
}
